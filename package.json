{
  "name": "basic-nodejs-mitso",
  "version": "1.0.0",
  "repository": {
    "type": "git",
    "url": ""
  },
  "type": "module",
  "description": "nodejs-mitso",
  "main": "dist/server.js",
  "scripts": {
    "start": "node dist/server.js",
    "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node src/index.ts",
    "build": "tsc",
    "lint": "eslint --ext .js,.ts --fix src/",
    "watch": "tsc -w"
  },
  "keywords": [
    "rest",
    "node",
    "express",
    "autotest",
    "starter"
  ],
  "license": "ISC",
  "engines": {
    "node": ">=18.0.0 <19"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFilesAfterEnv": [
      "./test/setup.js"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js": [
      "npm run lint"
    ]
  },
  "dependencies": {
    "@types/cors": "^2.8.18",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^9.0.0",
    "express": "^4.17.1",
    "express-validator": "^7.0.1",
    "uuid": "^8.3.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/connect-timeout": "^1.9.0",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^5.0.1",
    "@types/express-session": "^1.18.1",
    "@types/express-slow-down": "^1.3.5",
    "@types/express-validator": "^2.20.33",
    "@types/helmet": "^0.0.48",
    "@types/jsonwebtoken": "^9.0.9",
    "@types/morgan": "^1.9.9",
    "@types/node": "^22.15.17",
    "@types/uuid": "^10.0.0",
    "debug": "^4.3.1",
    "eslint": "^7.2.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "lint-staged": "^10.5.4",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
